<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BaseFilth">
    <defName>FloorWebsFilth</defName>
    <label>horrorwebs</label>
    <graphicData>
	  <graphicClass>Graphic_Random</graphicClass>
      <texPath>Things/Webs/HorrorWeb</texPath>
	  <drawSize>(3)</drawSize>
    </graphicData>
    <filth>
      <cleaningWorkToReduceThickness>35</cleaningWorkToReduceThickness>
    </filth>
  </ThingDef>

  <ThingDef ParentName="BaseFilth">
    <defName>FilthBloodHorror</defName>
    <label>bugblood</label>
    <statBases>
      <Beauty>-40</Beauty>
      <Cleanliness>-15</Cleanliness>
    </statBases>
    <graphicData>
      <texPath>Things/Filth/Spatter</texPath>
      <color>(60, 50, 40, 180)</color>
    </graphicData>
    <filth>
      <rainWashes>true</rainWashes>
      <cleaningWorkToReduceThickness>80</cleaningWorkToReduceThickness>
      <canFilthAttach>true</canFilthAttach>
    </filth>
  </ThingDef>

  <ThingDef ParentName="BaseFilth">
    <defName>FilthFireFoam</defName>
    <label>firefoam</label>
    <statBases>
      <Beauty>-25</Beauty>
      <Cleanliness>-5</Cleanliness>
    </statBases>
    <graphicData>
      <texPath>Things/Filth/Spatter</texPath>
      <color>(180, 180, 180, 100)</color>
    </graphicData>
    <pathCost>5</pathCost>
    <filth>
      <rainWashes>true</rainWashes>
      <cleaningWorkToReduceThickness>70</cleaningWorkToReduceThickness>
      <canFilthAttach>true</canFilthAttach>
      <allowsFire>false</allowsFire>
    </filth>
  </ThingDef>

  <ThingDef ParentName="BaseFilth">
    <defName>FilthAsh</defName>
    <label>ash</label>
    <statBases>
      <Beauty>-20</Beauty>
      <Cleanliness>-15</Cleanliness>
    </statBases>
    <graphicData>
      <texPath>Things/Filth/Ash</texPath>
      <color>(255, 255, 255, 220)</color>
    </graphicData>
    <filth>
      <rainWashes>true</rainWashes>
      <cleaningWorkToReduceThickness>70</cleaningWorkToReduceThickness>
      <canFilthAttach>true</canFilthAttach>
    </filth>
  </ThingDef>

</Defs>