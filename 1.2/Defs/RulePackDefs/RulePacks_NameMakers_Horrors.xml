<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RulePackDef Name="NamerAnimalBase" Abstract="True">
    <rulePack>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>animalnameunisex</keyword>
          <path>Names/Animal_Unisex</path>
        </li>
        <li Class="Rule_File">
          <keyword>animalnamemale</keyword>
          <path>Names/Animal_Male</path>
        </li>
        <li Class="Rule_File">
          <keyword>animalnamefemale</keyword>
          <path>Names/Animal_Female</path>
        </li>
        <li Class="Rule_NamePerson">
          <gender>Male</gender>
          <keyword>personnamemale</keyword>
        </li>
        <li Class="Rule_NamePerson">
          <gender>Female</gender>
          <keyword>personnamefemale</keyword>
        </li>
        <li Class="Rule_File">
          <keyword>badassconcept</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassanimal</keyword>
          <path>Words/Nouns/Animals_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassadjective</keyword>
          <path>Words/Adjectives/Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassperson</keyword>
          <path>Words/Nouns/People_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassconcept</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badasscolor</keyword>
          <path>Words/Nouns/Colors_Badass</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  <RulePackDef ParentName="NamerAnimalBase">
    <defName>NamerAnimalHorrors</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[exoticname]</li>
        <li>exoticname->[badassadjective]</li>
        <li>exoticname->[badassadjective]</li>
        <li>exoticname->[concept]</li>
        <li>exoticname->[concept]</li>
        <li>exoticname->[color]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
</Defs>
